{
    "name": "Container Name",
    "dockerComposeFile": "relative/path/to/docker-compose.yml",
    "service": "service to attach",
    "workspaceFolder": "/path/to/workdir",
    "runServices": [
        "services to run"
    ],
    "forwardPorts": [
        "ports to expose"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        // PHP
        "felixfbecker.php-intellisense",
        "felixfbecker.php-debug",
        "onecentlin.laravel5-snippets",
        "onecentlin.laravel-blade",
        "octref.vetur",

        // Python
        "ms-python.python",
        "ms-toolsai.jupyter",

        // Rust
        "rust-lang.rust"
    ]
}
